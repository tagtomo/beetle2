<%= form_for @addressee, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :number, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :number, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :data_type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :data_type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :zipno, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :zipno, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address1_cd, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address1_cd, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address2_cd, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address2_cd, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address3_cd, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address3_cd, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address4_cd, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address4_cd, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address5_cd, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address5_cd, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :address_kata, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :address_kata, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :start_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :start_date, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :end_date, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :end_date, :class => 'text_field' %>
    </div>
  </div>


  <%= f.submit nil, :class => 'btn btn-primary' %>
  <%= link_to 'Cancel', :back, :class => 'btn btn-default'  %>
  <%#= link_to t('.cancel', :default => t("helpers.links.cancel")),
            addressees_path, :class => 'btn btn-default' %>

<% end %>
